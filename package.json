{
  "name": "kangoo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/pickers": "^3.3.10",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "cra-append-sw": "^2.7.0",
    "date-fns": "^2.23.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "build": "cross-env BUILD_PATH='./functions/build' react-scripts build && cra-append-sw --skip-compile ./functions/build/custom-sw.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "nodemon ./server/app.js",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run start\"",
    "client:install": "npm run install",
    "client:build": "npm run build",
    "deploy:dev": "firebase use dev && npm run build && firebase deploy --only 'functions,hosting'",
    "serve:dev": "firebase use dev && npm run build && firebase serve --only 'functions,hosting'",
    "deploy:prod": "firebase use prod && npm run build && firebase deploy --only 'functions,hosting'",
    "serve:prod": "firebase use prod && npm run build && firebase serve --only 'functions,hosting'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "nodemon": "^2.0.12"
  }
}
